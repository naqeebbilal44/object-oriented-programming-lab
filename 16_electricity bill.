#include <iostream>
using namespace std;

class Bill {
private:
    int units;

public:
    // Parameterized constructor
    Bill(int u) {
        units = u;
    }

    // Copy constructor
    Bill(const Bill &b) {
        units = b.units;
    }

    int totalBill() {
        return units * 5;
    }

    void display() {
        cout << "Units: " << units << endl;
        cout << "Total Bill: â‚¹" << totalBill() << endl;
    }
};

int main() {
    Bill b1(120);   // original
    Bill b2 = b1;   // copied

    cout << "Original Bill:" << endl;
    b1.display();

    cout << "\nCopied Bill:" << endl;
    b2.display();

    return 0;
}